
{
  "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "pretty": true,
      "sourceMap": true,
      "target": "es5",
      "experimentalDecorators": false,
      "emitDecoratorMetadata": false,
      "listEmittedFiles": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "skipDefaultLibCheck": false,
      "baseUrl": "./src"
  },
  "include": [
      "./**/*.ts"
  ],
  "exclude": [
      "node_modules",
      ".vscode"
  ]
}